{
  "servers": {
    "nuxt": {
      "type": "http",
      "url": "https://nuxt.com/mcp"
    },
    "nuxt-ui": {
      "type": "http",
      "url": "https://ui.nuxt.com/mcp"
    },
    "tailwindcss-server": {
      "command": "npx",
      "args": ["-y", "tailwindcss-mcp-server@latest"]
    },
    "typescript": {
      "command": "npx",
      "args": ["-y", "typescript-mcp@latest"]
    },
    "bun": {
      "command": "bunx",
      "args": ["-y", "mcp-bun@latest"],
      "env": {
        "DISABLE_NOTIFICATIONS": "true"
      }
    },
    "git": {
      "command": "npx",
      "args": ["-y", "@mseep/git-mcp-server@latest"]
    }
  }
}
